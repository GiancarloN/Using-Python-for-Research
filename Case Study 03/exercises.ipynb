    "def distance(p1, p2):\n",
    "    \"\"\"Find the distances between points p1 and p2.\"\"\"\n",
    "\n",
    "    return np.sqrt(np.sum(np.power(p2 - p1, 2)))\n",
    "\n",
    "p1 = np.array([1, 1])\n",
    "p2 = np.array([4, 4])\n",
    "\n",
    "d = distance(p1, p2)"
